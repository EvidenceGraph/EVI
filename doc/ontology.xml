    <!-- http://schema.org/Article -->


    <owl:Class rdf:about="http://schema.org/Article">
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Representation"/>
        <rdfs:comment>An identifiable source object of which an EvidenceGraph is derived. It includes information about its location, time of creation, and the authors.</rdfs:comment>
        <rdfs:label>Article</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/Claim -->


    <owl:Class rdf:about="http://schema.org/Claim">
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Representation"/>
        <rdfs:comment>A statement argued in a scientific experiment.</rdfs:comment>
        <rdfs:label>Claim</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/Dataset -->


    <owl:Class rdf:about="http://schema.org/Dataset">
        <owl:equivalentClass rdf:resource="https://w3id.org/evi/Dataset"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Representation"/>
        <rdfs:comment>A body of structured information describing some topic(s) of interest.</rdfs:comment>
        <rdfs:label>Dataset</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/Organization -->


    <owl:Class rdf:about="http://schema.org/Organization">
        <rdfs:subClassOf rdf:resource="https://www.w3.org/TR/2013/REC-prov-o-20130430/#Agent"/>
        <rdfs:comment>An organization is a social or legal institution such as a company, society, etc.</rdfs:comment>
        <rdfs:label>Organization</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/Person -->


    <owl:Class rdf:about="http://schema.org/Person">
        <rdfs:subClassOf rdf:resource="https://www.w3.org/TR/2013/REC-prov-o-20130430/#Agent"/>
        <rdfs:comment>Person agents are people.</rdfs:comment>
        <rdfs:label>Person</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/Service -->


    <owl:Class rdf:about="http://schema.org/Service">
        <owl:equivalentClass rdf:resource="https://w3id.org/evi/Service"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Method"/>
        <rdfs:comment>A Method used during a Computation Activity.</rdfs:comment>
        <rdfs:label>Service</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/SoftwareApplication -->


    <owl:Class rdf:about="http://schema.org/SoftwareApplication">
        <owl:equivalentClass rdf:resource="https://w3id.org/evi/Software"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Material"/>
        <rdfs:comment>A set of instructions to perform specific Computation Activity.</rdfs:comment>
        <rdfs:label>SoftwareApplication</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/evi/Caption -->


    <owl:Class rdf:about="https://w3id.org/evi/Caption">
        <rdfs:subClassOf rdf:resource="http://schema.org/Claim"/>
        <rdfs:comment>Composed of descriptive statements which describe information about objects such as images, tables, datasets.</rdfs:comment>
        <rdfs:label>Caption</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/evi/Computation -->


    <owl:Class rdf:about="https://w3id.org/evi/Computation">
        <rdfs:subClassOf rdf:resource="https://www.w3.org/TR/2013/REC-prov-o-20130430/#Activity"/>
        <rdfs:comment>Computation is an Activity which is generally performed on a Dataset with the help of a SoftwareApplication</rdfs:comment>
        <rdfs:label>Computation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/evi/Dataset -->


    <owl:Class rdf:about="https://w3id.org/evi/Dataset">
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Representation"/>
        <rdfs:comment>A body of structured information describing some topic(s) of interest.</rdfs:comment>
        <rdfs:label>Dataset</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/evi/EvidenceGraph -->


    <owl:Class rdf:about="https://w3id.org/evi/EvidenceGraph">
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Representation"/>
        <rdfs:comment>An EvidenceGraph is a set of Representation, having supportive and/or challenging relations to one another.</rdfs:comment>
        <rdfs:label>EvidenceGraph</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/evi/Material -->


    <owl:Class rdf:about="https://w3id.org/evi/Material">
        <rdfs:subClassOf rdf:resource="https://w3id.org/evi/Representation"/>
