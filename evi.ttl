@prefix : <https://w3id.org/EVI#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/EVI> .

<https://w3id.org/EVI> rdf:type owl:Ontology ;
                        <http://purl.org/dc/terms/contributor> "https://github.com/jniestroy" ,
                                                               "https://github.com/mlev71" ;
                        <http://purl.org/dc/terms/created> "2020-07-27"@en ;
                        <http://purl.org/dc/terms/creator> "https://datascience.virginia.edu/people/tim-clark" ,
                                                           "https://github.com/sadnanalmanir" ;
                        <http://purl.org/dc/terms/description> "The Evidence Graph ontology extends core concepts from the W3C Provenance Ontology PROV-O and Bioschemas' Profiles to describe evidence for correctness of findings in biomedical publications. The semantic data model in EVI is expressed using OWL2 Web Ontology Language (OWL2)."@en ;
                        <http://purl.org/dc/terms/license> "http://creativecommons.org/licenses/by-nc-sa/2.0/" ;
                        <http://purl.org/dc/terms/title> "EVI: Evidence Graph Ontology"@en ;
                        <http://purl.org/vocab/vann/preferredNamespacePrefix> "evi" ;
                        <http://purl.org/vocab/vann/preferredNamespaceUri> "http://w3id.org/EVI"@en ;
                        owl:versionIRI "http://www.w3.org/2002/07/owl"@en ;
                        owl:versionInfo "0.1"@en .

